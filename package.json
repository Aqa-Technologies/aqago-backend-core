  },
  "keywords": [
    "aqago"
  ],
  "author": "Aqa Engineering Team",
  "license": "BSL-1.0",
  "devDependencies": {
    "chai": "^5.1.2",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "jose": "^5.9.6",
    "node-forge": "^1.3.1",
    "prettier": "^3.4.2"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
